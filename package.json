{
  "name": "sp-next-starter",
  "description": "A simple starter to get up and developing quickly with Next.js",
  "version": "1.0",
  "private": true,
  "author": "Alex <dev@salex.pro>",
  "dependencies": {
    "@next/eslint-plugin-next": "^15.1.4",
    "@next/third-parties": "^15.3.1",
    "@nimpl/classnames-minifier": "^4.0.0",
    "@svgr/webpack": "^8.0.0",
    "@vercel/analytics": "^1.1.2",
    "bootstrap": "^5.3.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "next": "^15.3.1",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^19.1.0",
    "sass": "^1.83.0",
    "sharp": "^0.34.0"
  },
  "scripts": {
    "build": "NODE_OPTIONS='--no-network-family-autoselection' next build",
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "start": "NODE_OPTIONS='--no-network-family-autoselection' next dev",
    "serve": "next start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,mdx,md,css,scss}\"",
    "lint": "eslint .",
    "copy-bs-settings": "node ./scripts/copy-bs-settings.js",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky && pnpm copy-bs-settings",
    "cz": "cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/js": "^9.10.0",
    "@next/bundle-analyzer": "15.5.6",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.10.0",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.12.0",
    "stylelint-config-recess-order": "^7.0.0",
    "stylelint-config-standard-scss": "^15.0.0",
    "stylelint-prettier": "^5.0.2",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.18.1"
  },
  "packageManager": "pnpm@10.22.0"
}
